{
  "name": "ctrlc-windows",
  "version": "1.0.2",
  "description": "Send CTRL-C to a process on Windows",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thefrontside/ctrlc-windows.git"
  },
  "files": [
    "not-windows.js",
    "build.js",
    "native",
    "lib"
  ],
  "binary": {
    "module_name": "index",
    "host": "https://github.com/thefrontside/ctrlc-windows/releases/download/",
    "remote_path": "v{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./dist"
  },
  "author": "Frontside Engineering <engineering@frontside.com>",
  "license": "MIT",
  "dependencies": {
    "neon-cli": "^0.5.0",
    "node-pre-gyp": "^0.16.0"
  },
  "scripts": {
    "build": "node build.js",
    "clean": "node clean.js",
    "install": "node not-windows.js || node-pre-gyp install --fallback-to-build=false || node build.js release",
    "test": "mocha -r ts-node/register \"test/**/*.test.ts\""
  },
  "devDependencies": {
    "@frontside/tsconfig": "^1.1.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "expect": "^26.6.2",
    "mocha": "^8.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "@changesets/cli": "2.7.2"
  },
  "volta": {
    "node": "12.19.0",
    "yarn": "1.22.10"
  }
}
